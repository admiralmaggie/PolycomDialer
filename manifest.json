{
    "manifest_version": 2,
    "name": "Polycom Dialer",
    "description": "Dials a phone number Polycom VVX (Firmware 5.9.0 and above)",
    "version": "1.0",
    "author": "Aaron Rouhi",
    "permissions": [
        "contextMenus",
		"https://*/*",
		"storage"
    ],
	"options_ui": {
    "page": "options.html",
    "open_in_tab": true
	},
	"icons": { "16": "icon16.png",
			   "32": "icon32.png",
               "128": "icon128.png" },
    "background": {	
        "persistent": false,
        "scripts": [
            "index.js"
        ]
    }
}